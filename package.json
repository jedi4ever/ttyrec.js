      "grunt": "~0.4.1",
      "grunt-contrib-jshint": "~0.6.0",
      "grunt-contrib-watch": "~0.4.4",
      "grunt-release": "~0.3.5",
      "grunt-html": "~0.3.3",
      "grunt-readme-generator": "~0.3.41",
      "grunt-contrib-clean": "~0.5.0",
      "grunt-jsdoc": "~0.4.0",
      "grunt-gh-pages": "~0.7.1",
